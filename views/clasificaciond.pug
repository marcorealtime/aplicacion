doctype html
html(ng-app='sampleAppd1')
    head
        meta(charset='utf-8')
        meta(name='viewport', content='width=device-width, initial-scale=1.0')
        title GO-TARIJA
        link(rel='stylesheet', href='https://cdnjs.cloudflare.com/ajax/libs/bootswatch/3.3.7/cosmo/bootstrap.min.css')
        link(rel='stylesheet', href='https://fonts.googleapis.com/css?family=Source+Sans+Pro:300,400,700')
        link(rel='stylesheet', href='/stylesheets/styles.min.css')
        link(rel='stylesheet', href='/stylesheets/style.css')
    body
        nav.navbar.navbar-default
            .container
                .navbar-header
                    a.navbar-brand.navbar-link(href='#')
                        h3.tipografia3 GO-TARIJA
                    button.navbar-toggle.collapsed(data-toggle='collapse', data-target='#navcol-1')
                        span.sr-only Toggle navigation
                        span.icon-bar
                        span.icon-bar
                        span.icon-bar
                #navcol-1.collapse.navbar-collapse
                    ul.nav.navbar-nav.navbar-right
                        li(role='presentation')
                            a(href='/') INICIO
                        li(role='presentation')
                            a(href='/piloto') PILOTOS
                        li(role='presentation')
                            a(href='/ranking') RANKING
                        li(role='presentation')
                            a(href='/calendario') CALENDARIO
                        li.active(role='presentation')
                            a.dropdown-toggle(data-toggle='dropdown', href='#')
                                | DIRECTO
                                span.caret
                            ul.dropdown-menu
                                li(role='presentation')
                                    a(href='/inscritos') INSCRITOS
                                li(role='presentation')
                                    a(href='/orden') ORDEN DE LARGADA
                                li(role='presentation')
                                    a(href='/clasificacion') ETAPAS/PRUEBAS ESPECIALES
                                li(role='presentation')
                                    a(href='/carrera') TIEMPOS GENERAL
                        li(role='presentation')
                            a.dropdown-toggle(data-toggle='dropdown', href='#')
                                | MOTOS
                                span.caret
                            ul.dropdown-menu
                                li(role='presentation')
                                    a(href='/inscritosmoto') INSCRITOS
                                li(role='presentation')
                                    a(href='/clasificacionmoto') ETAPAS/PRUEBAS ESPECIALES
                                li(role='presentation')
                                    a(href='/carreramoto') TIEMPOS GENERAL
                        li(role='presentation')
                            a(href='#') GALERIA
        .container
            .row(ng-controller='SampleCtrlcatd1')
                .col-md-4.col-md-push-7.phone-preview
                    .iphone-mockup
                .col-md-6.col-md-pull-3.get-it
                    h4.tipografia2  PRUEBAS ESPECIALES : #{carrera}
                    h4.tipografia2 ETAPA:
                    select(name='singleSelect', ng-model='data.singleSelect', ng-change='LoadSessionData(data.singleSelect)', style='width:100%')
                        option(value='', disabled='', selected='', class='timpografia2') Seleccionar Etapa
                        option(ng-repeat='x1 in etapas') {{x1.orden}}
                    hr
                    h4.tipografia2 PRUEBAS ESPECIALES:
                    select(name='singleSelectP', ng-model='data.singlePE', ng-change='LoadSessionPespecial(data.singlePE)', style='width:100%')
                        option(value='', disabled='', selected='', class='timpografia2') Seleccionar Prueba Esp.
                        option(ng-repeat='x1 in especiales') {{x1.nombre}}
                    hr
                    .panel.panel-default(ng-repeat='x1 in unitscorriendo')
                        .panel-heading(style="background-color: #{{x1.color}};")
                            h3.panel-title
                                center
                                    h5.tipografia1 {{x1.estado}}
                                h5.tipografia1 {{unitscorriendo.indexOf(x1)+1}}
                                    p.pull-right {{x1.clase}}
                                h5.tipografia1 
                                    span.icon-key2
                                    | {{x1.auto}} 
                                h5.tipografia1
                                    span.icon-user
                                    | {{x1.nombre}}
                                h5.tipografia1
                                    span.icon-user-plus
                                    | {{x1.nave}}
                                center
                                    h2  
                                        #reloj
                                            | {{x1.tiempo}}
                                #reloj2
                                    p.pull-right +{{x1.tiempoa}}
                                br
                                #reloj3
                                    p.text-warning Pena :{{x1.pena}}
                        .panel-body
                            a.btn.btn-primary.btn-xs(href='#cor{{unitscorriendo.indexOf(x1)+1}}', data-toggle='collapse' style='width:100%') INFO
                            .collapse(id='cor{{unitscorriendo.indexOf(x1)+1}}')
                                br
                                #reloj3
                                    p Velocidad:  {{x1.km}} km/h       
                                    p Dif. C/u:  {{x1.tiempoa}}
                                    p Dif 1° C/u:  {{x1.tiempop}}
                    .panel.panel-default(ng-repeat='x1 in units')
                        .panel-heading(style="background-color: #{{x1.color}};")
                            h3.panel-title
                                center
                                    h5.tipografia1 {{x1.estado}}
